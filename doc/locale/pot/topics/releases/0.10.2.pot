msgstr ""

#: ../../topics/releases/0.10.2.rst:118
# 741cfbb93bba42e1993264c49c314ce2
msgid "A new Augeas module by Ulrich Dangel for editing and verifying config files."
msgstr ""

#: ../../topics/releases/0.10.2.rst:121
# cd97f8f879014a3fb6f3db5b867bfa45
msgid "Native Debian Service module"
msgstr ""

#: ../../topics/releases/0.10.2.rst:123
# 590b7c6ebcf1422da22bb5cdd984f282
msgid "The support for the Debian was further improved with an new service module for Debian by Ahmad Khayyat supporting `disable` and `enable`."
msgstr ""

#: ../../topics/releases/0.10.2.rst:128
