msgstr ""

#: ../../topics/releases/0.10.2.rst:130
# 1358c627afb2457ea91e0e313a762eca
msgid "Cassandra support has been added by Adam Garside. Currently only status and diagnostic information are supported."
msgstr ""

#: ../../topics/releases/0.10.2.rst:134
# f5ba27b0e54243b0894dd4604e1f6184
msgid "Networking"
msgstr ""

#: ../../topics/releases/0.10.2.rst:136
# 9ad43823c9954a97bb3c73da28518b6b
msgid "The networking support for `RHEL` has been improved and supports bonding support as well as zeroconf configuration."
msgstr ""

#: ../../topics/releases/0.10.2.rst:141
# d9237326ee074b19875da85c862ed7b6
msgid "Monit"
msgstr ""

#: ../../topics/releases/0.10.2.rst:143
# 600900e8a1f04a00927e30d490d2f184
