msgstr ""

#: ../../../salt/states/ssh_auth.pydocstring of salt.states.ssh_auth.absent:4
#: ../../../salt/states/ssh_auth.pydocstring of salt.states.ssh_auth.present:4
msgid "The ssh key to manage"
msgstr ""

#: ../../../salt/states/ssh_auth.pydocstring of salt.states.ssh_auth.absent:7
#: ../../../salt/states/ssh_auth.pydocstring of salt.states.ssh_auth.present:7
