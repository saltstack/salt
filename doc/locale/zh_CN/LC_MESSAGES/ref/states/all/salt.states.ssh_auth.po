msgid "salt.states.ssh_auth"
msgstr ""

#: ../../../salt/states/ssh_auth.pydocstring of salt.states.ssh_auth:2
msgid "Control of entries in SSH authorized_key files."
msgstr ""

#: ../../../salt/states/ssh_auth.pydocstring of salt.states.ssh_auth:4
msgid ""
"The information stored in a user's ssh authorized key file can be easily "
"controlled via the ssh_auth state. Defaults can be set by the enc, options, "
"and comment keys. These defaults can be overridden by including them in the "
"name."
msgstr ""

#: ../../../salt/states/ssh_auth.pydocstring of salt.states.ssh_auth.absent:1
msgid "Verifies that the specified ssh key is absent"
msgstr ""

