#: ../../topics/releases/0.16.3.rst:6
msgid "Salt 0.16.3 Release Notes"
msgstr ""

#: ../../topics/releases/0.16.3.rst:8
msgid ""
"Version 0.16.3 is another bugfix release for :doc:`0.16.0 "
"</topics/releases/0.16.0>`. The changes include:"
msgstr ""

#: ../../topics/releases/0.16.3.rst:11
msgid "Various documentation fixes"
msgstr ""

#: ../../topics/releases/0.16.3.rst:12
msgid "Fix proc directory regression (:issue:`6502`)"
msgstr ""

#: ../../topics/releases/0.16.3.rst:13
msgid "Properly detect Linaro_ Linux (:issue:`6496`)"
msgstr ""

#: ../../topics/releases/0.16.3.rst:14
msgid ""
"Fix regressions in :mod:`mount.mounted <salt.states.mount.mounted>` "
"(:issue:`6522`, :issue:`6545`)"
msgstr ""

#: ../../topics/releases/0.16.3.rst:16
msgid "Skip malformed state requisites (:issue:`6521`)"
msgstr ""

#: ../../topics/releases/0.16.3.rst:17
msgid "Fix regression in gitfs from bad import"
msgstr ""

#: ../../topics/releases/0.16.3.rst:18
msgid ""
"Fix for watching prereq states (including recursive requisite error) "
"(:issue:`6057`)"
msgstr ""

#: ../../topics/releases/0.16.3.rst:20
msgid "Fix mod_watch not overriding prereq (:issue:`6520`)"
msgstr ""

#: ../../topics/releases/0.16.3.rst:21
msgid ""
"Don't allow functions which compile states to be called within states "
"(:issue:`5623`)"
msgstr ""

#: ../../topics/releases/0.16.3.rst:23
msgid "Return error for malformed top.sls (:issue:`6544`)"
msgstr ""

#: ../../topics/releases/0.16.3.rst:24
msgid "Fix traceback in :mod:`mysql.query <salt.modules.mysql.query>`"
msgstr ""

#: ../../topics/releases/0.16.3.rst:25
msgid ""
"Fix regression in binary package installation for 64-bit packages on Debian-"
"based Linux distros (:issue:`6563`)"
msgstr ""

#: ../../topics/releases/0.16.3.rst:27
msgid ""
"Fix traceback caused by running :mod:`cp.push <salt.modules.cp.push>` "
"without having set :conf_master:`file_recv` in the master config file"
msgstr ""

#: ../../topics/releases/0.16.3.rst:29
msgid "Fix scheduler configuration in pillar (:issue:`6201`)"
msgstr ""
